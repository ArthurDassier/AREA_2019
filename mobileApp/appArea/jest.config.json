{
	"preset": "jest-expo",
	"collectCoverage": true,
	"moduleDirectories": [
		"node_modules",
		"screens"
	],
	"transform": {
		"^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
	},
	"setupFiles": [
		"<rootDir>/jest/setup.js"
	],
	"coveragePathIgnorePatterns": [
		"/node_modules/",
		"/jest"
	]
}